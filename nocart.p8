pico-8 cartridge // http://www.pico-8.com
version 38
__lua__
--isometric tiles
--https://txgruppi.com
grid={}
m={}
sel={}
xstep=7
ystep=4
tx,ty = 0,0

function _init()
 poke(0x5f2d, 1)

	for y=0,20 do
  offset=0
  if y%2==1 then
   offset=1
  end
  for x=0,7-offset do
	  sp=1
	  if flr(rnd(5)) == 1 then
	   sp=3
	  end

			local til= {
			 index=#m,
    sp=sp,
    offset=offset,
    x=8+x*xstep*2+(offset*xstep),
    y=30+y*ystep,
    z=1
   }
   add(m, til)
   grid[x .. ',' .. y] = til 
  end
 end
 
 buildings=flr(rnd(30))
 
 z = 1
 
 for i=1,buildings do
  height = flr(rnd(6))+1
  
  x = flr(rnd(7))
  y = flr(rnd(19))
  z = 1
  
  offset=1
  if y%2==1 then
   offset=0
  end
  
  for h=1,height do
	  z = z + 1
	 	add(m, {
	 		sp=5,
	 		offset=offset,
	 		x=8+x*xstep*2+(offset*xstep),
	   y=30+y*ystep-(3*z+2),
	   z=z
	  })
  end
 end

end

function _draw()
 cls(1)
 for _, t in ipairs(m) do
 	spr(
    t.sp,
    t.x,
    t.y,
    2,
    2
   )
  // should we highlight
  if sel[_] then
    spr(33,
      t.x,
      t.y,
      2,
      2
    )
  end
 end
 
	local mx, my = stat(32)-1, stat(33)-1
 spr(65, mx, my)
 
 print(tx .. ',' .. ty)
end

function _update()

 clr_sels()
	local mx, my = stat(32), stat(33)
 mx, my = mx, my-30
 ty = flr(my/ystep)
 offset = 0
 if ty%2==1 then
 	offset = 1
 end
 tx = flr((mx-offset*xstep)/(xstep*2))
 local til = grid[tx .. ',' .. ty]
 if til then
   sel[til.index] = true
 end
 
end

function clr_sels()
 sel = {}
end
